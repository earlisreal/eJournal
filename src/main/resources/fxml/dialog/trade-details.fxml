         <TableColumn fx:id="logShares" prefWidth="74.0" text="Shares"/>
         <TableColumn fx:id="logFees" prefWidth="74.0" text="Fees"/>
         <TableColumn fx:id="logNet" prefWidth="83.0" text="Net Amount"/>
         <TableColumn fx:id="logStrategy" prefWidth="131.0" text="Strategy"/>
      </columns>
   </TableView>
   <TableView layoutX="14.0" layoutY="14.0" prefHeight="329.0" prefWidth="200.0">
      <columns>
         <TableColumn prefWidth="100.0" text="Statistic"/>
         <TableColumn prefWidth="100.0" text="Value"/>
      </columns>
   </TableView>
</AnchorPane>
